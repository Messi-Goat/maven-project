pipelline {
	agent any 
	
	stages {
				stage('scm checkout'){
				{
				steps
				{
				git branch: 'master', url: 'https://github.com/Messi-Goat/maven-project.git
				
				}
				}
				
				stage('parallel execution')
				{
				parallel
				{
				stage('package')
				{
				steps
				{
				 withMaven(jdk: 'localjdk', maven: 'localmaven')
				 {   
                    sh 'mvn compile'} 
                }
               }
			   stage('compile')
			   {
			   steps{
			    withMaven(jdk: 'localjdk', maven: 'localmaven') 
                {   
                    sh 'mvn compile'}
                }
               }
			   }
			   }
			   }
			   }
              
